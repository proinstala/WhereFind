    "version": "0.2.0",
    "configurations": [
      {
          "type"        : "msedge",
          "request"     : "launch",
          "name"        : "Edge : Lanzar",
          "url"         : "${config:Tomcat.Url}",
          "webRoot"     : "${workspaceFolder}"
      },

      {
        "type"          : "msedge",
        "request"       : "launch",
        "name"          : "Edge : Clean-Compile-Run",
        "url"           : "${config:Tomcat.Url}",
        "webRoot"       : "${workspaceFolder}",
        "preLaunchTask" : "mavenCleanInstall",
      },

      {
        "type"          : "msedge",
        "request"       : "launch",
        "name"          : "Edge : Compile-Run",
        "url"           : "${config:Tomcat.Url}",
        "webRoot"       : "${workspaceFolder}",
        "preLaunchTask" : "mavenInstall",
      },

      {
        "type"          : "chrome",
        "request"       : "launch",
        "name"          : "Chrome : Lanzar",
        "url"           : "${config:Tomcat.Url}",
        "webRoot"       : "${workspaceFolder}"
      },

      {
        "type"          : "chrome",
        "request"       : "launch",
        "name"          : "Chrome : Clean-Compile-Run",
        "url"           : "${config:Tomcat.Url}",
        "webRoot"       : "${workspaceFolder}",
        "preLaunchTask" : "mavenCleanInstall",
      },

      {
        "type"          : "chrome",
        "request"       : "launch",
        "name"          : "Chrome : Compile-Run",
        "url"           : "${config:Tomcat.Url}",
        "webRoot"       : "${workspaceFolder}",
        "preLaunchTask" : "mavenInstall",
      }
    ],
}