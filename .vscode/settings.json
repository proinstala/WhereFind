{
    // La uri que contiene el proyecto
    "Tomcat.Proyecto" : "WhereFind",

    // La url en la que se encuentra el servidor web tomcat
    "Tomcat.Url": "http://localhost:8080/${config:Tomcat.Proyecto}/",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/.history/*": true,
        "**/.git/*": true,
        "**/*.meta": true,
        "**/*.*.meta": true,
        "**/*.unity": true,
        "**/*.unityproj": true,
        "**/*.mat": true,
        "**/*.fbx": true,
        "**/*.FBX": true,
        "**/*.tga": true,
        "**/*.cubemap": true,
        "**/*.prefab": true,
        "**/Library": true,
        "**/ProjectSettings": true,
        "**/Temp": true,
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true,
        "**/.vs": true,
        "**/.vscode": false,
        "**/*.lfm.bak": true,
        "**/*.lps.bak": true,
        "**/*.lpi.bak": true,
        "**/*.pas.bak": true,
        "**/backup": true,
        "target": true
    },
    "explorerExclude.backup": {},
